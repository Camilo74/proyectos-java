<div id="element-${id}">
	<div>
		<a href="#" onclick="contract('${id}','${hostip}','${hostname}','${osname}','${acount}')">(-)</a>
		<label>${hostip}</label>
		<label>(${hostname}:${osname})</label>
		<label>(${acount})</label>
	</div>
	<div id="element-${id}-files" class="element" ondragover="fileOnDragOver(this)" ondragend="fileOnDragEnd(this)" ondrop="fileOnDrop">
		${files}
	</div>
	<br>
</div>