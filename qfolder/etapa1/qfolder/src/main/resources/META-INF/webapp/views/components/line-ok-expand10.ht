<div id="element-${id}">
	<div id="header-${id}">
		<a href="#" onclick="contract('${id}','${hostip}','${hostname}','${osname}','${acount}')"><img src="/img/less.png"></img></a>
		<a href="${hostip}/ws/status">${hostip}</a>
		<label class="namespace">(${hostname}:${osname})</label>
		<label>(${acount})</label>
	</div>
	<div id="content-${id}" class="element" ondragover="fileOnDragOver(this)" ondragend="fileOnDragEnd(this)" ondrop="fileOnDrop(event,'${id}','${hostip}','${hostname}','${osname}','${acount}')">
		${files}
	</div>
	<br id="footer-${id}"/>
</div>